{
  "compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/context/*": ["./src/utils/context/*"],
			"@/gen/*": ["./src/components/gen/*"],
			"@/db/*": ["./src/utils/db/*"],
			"@/dynamic/*": ["./src/components/pages/*/index.tsx"],
			"@/wrapper/*": ["./src/components/wrapper/*"],
			"@/serverSession": [".src/exports/getServerSession.ts"],
			"@/axios": ["./src/utils/axios/index.ts"],
			"@/public/*": ["public/*"],
			"@/styles/*": ["src/styles/*"],
			"@/types/*": ["./src/types/*"],
			"@/fontello/*": ["./public/media/fontello/*"],
		}
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
